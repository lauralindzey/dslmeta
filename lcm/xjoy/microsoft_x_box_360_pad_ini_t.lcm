package xjoy;

struct microsoft_x_box_360_pad_ini_t
{
  // The standard name of the joystick;
  string xbox_device_name;

  // the joystick os dev path
  string os_device;

  // Where to publish the data
  string channel_data;

  // The take channel to determine whose joystick is active.
  string channel_joytake;

  // Hostname where it will run
  string host; 
}
